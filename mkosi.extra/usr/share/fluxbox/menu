[begin] (Greetings)
[encoding] {UTF-8}
    [exec] (Launch...) {rofi -show drun}
    [exec] (Web Browser) {waterfox}
    [exec] (File Manager) {thunar}
[submenu] (Accessories)
    [exec] (Media Player) {vlc}
    [exec] (Text Editor) {mousepad}
    [exec] (Terminal) {alacritty}
    [exec] (Manual Pages) {mangl}
[end]
[submenu] (Settings)
    [exec] (Volume Control) {pavucontrol}
    [exec] (Network Connection) {nm-connection-editor}
[submenu] (Window Manager)
    [config] (Configure)
[submenu] (System Styles) {Choose a style...}
    [stylesdir] (/usr/share/fluxbox/styles)
[end]
[submenu] (User Styles) {Choose a style...}
    [stylesdir] (~/.fluxbox/styles)
[end]
    [workspaces] (Workspace List)
    [commanddialog] (Fluxbox Command)
    [reconfig] (Reload Config)
    [restart] (Restart Window Manager)
    [separator]
    [exit] (Exit Session)
[end]
[end]
[submenu] (Power)
    [exec] (Power off) {xmessage -center -buttons OK:0,Cancel:1 This will exit your session and power off your computer. && systemctl poweroff}
    [exec] (Reboot) {xmessage -center -buttons OK:0,Cancel:1 This will exit your session and reboot your computer. && systemctl reboot}
    [exec] (Sleep) {xmessage -center -buttons OK:0,Cancel:1 Your computer will be eepy. && systemctl sleep}
[end]
[endencoding]
[end]
